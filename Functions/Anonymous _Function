/*The function doesn't having any name are called anonymous*/
//With parameter
console.log(function (a,b)
{
    return a*b;
}(4,5));
//without parameter
(function()
{
    console.log("Hello");
}())
/*function doesn't having any name
The variable greet is used to store the function
function expression->function is assigned to some variable for eg-greet=function()
mostly used with anonymous but can be use with named function also*/
greet=function()
{
  console.log("Hello");
}
